        android:visibility="gone"
        app:civ_border_width="@dimen/dp1"
        app:layout_constraintEnd_toStartOf="@id/fragment_on_play_home__player_8"
        app:layout_constraintTop_toBottomOf="@id/fragment_on_play_home__player_6"
        tools:visibility="visible" />

    <de.hdodenhof.circleimageview.CircleImageView
        android:id="@+id/fragment_on_play_home__player_8"
        android:layout_width="@dimen/dp55"
        android:layout_height="@dimen/dp55"
        android:layout_margin="@dimen/dp5"
        android:src="@mipmap/user34"
