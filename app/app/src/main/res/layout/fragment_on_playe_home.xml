<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/primary_slow_purple"
    android:padding="@dimen/dp10"
    tools:context=".ui.fragments.OnPlayeHomeFragment">

    <TextView
        android:id="@+id/fragment_on_play_home__title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:text="@string/que_empieze_la_fiesta"
        android:textColor="@color/primary_hard_purple"
        android:textSize="@dimen/sp50"
        android:paddingBottom="@dimen/dp20"
        app:layout_constraintTop_toTopOf="parent" />

    <de.hdodenhof.circleimageview.CircleImageView
        android:id="@+id/fragment_on_play_home__player_1"
        android:layout_width="@dimen/dp55"
        android:layout_height="@dimen/dp55"
        android:layout_margin="@dimen/dp5"
        android:src="@mipmap/user12"
        android:visibility="gone"
        app:civ_border_width="@dimen/dp1"
        app:layout_constraintEnd_toStartOf="@id/fragment_on_play_home__player_2"
        app:layout_constraintTop_toBottomOf="@id/fragment_on_play_home__title"
        tools:visibility="visible" />

    <de.hdodenhof.circleimageview.CircleImageView
        android:id="@+id/fragment_on_play_home__player_2"
        android:layout_width="@dimen/dp55"
        android:layout_height="@dimen/dp55"
        android:layout_margin="@dimen/dp5"
        android:src="@mipmap/user11"
        android:visibility="gone"
        app:civ_border_width="@dimen/dp1"
        app:layout_constraintEnd_toStartOf="@id/fragment_on_play_home__player_3"
        app:layout_constraintTop_toBottomOf="@id/fragment_on_play_home__title"
        tools:visibility="visible" />

    <de.hdodenhof.circleimageview.CircleImageView
        android:id="@+id/fragment_on_play_home__player_3"
        android:layout_width="@dimen/dp55"
        android:layout_height="@dimen/dp55"
        android:layout_margin="@dimen/dp5"
        android:src="@mipmap/user10"
        android:visibility="gone"
        app:civ_border_width="@dimen/dp1"
        app:layout_constraintEnd_toStartOf="@id/fragment_on_play_home__player_4"
        app:layout_constraintTop_toBottomOf="@id/fragment_on_play_home__title"
        tools:visibility="visible" />

    <de.hdodenhof.circleimageview.CircleImageView
        android:id="@+id/fragment_on_play_home__player_4"
        android:layout_width="@dimen/dp55"
        android:layout_height="@dimen/dp55"
        android:layout_margin="@dimen/dp5"
        android:src="@mipmap/user9"
        android:visibility="gone"
        app:civ_border_width="@dimen/dp1"
        app:layout_constraintEnd_toStartOf="@id/fragment_on_play_home__player_5"
        app:layout_constraintTop_toBottomOf="@id/fragment_on_play_home__title"
        tools:visibility="visible" />

    <de.hdodenhof.circleimageview.CircleImageView
